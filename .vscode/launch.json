{
 "version": "0.2.0",
 "configurations": [
   {
     "type": "node",
     "request": "launch",
     "name": "Launch Program with ts-node",
     "skipFiles": [
       "<node_internals>/**"
     ],
     "runtimeArgs": [
       "-r",
       "ts-node/register/transpile-only"
     ],
     "args": [
       "${workspaceFolder}/src/index.ts"
     ],
     "sourceMaps": true,
     "smartStep": true,
     "env": {
       "NODE_ENV": "development"
     }
   }
 ]
}